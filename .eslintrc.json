{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "commonjs",
		"ecmaVersion": 2017
	},
	"globals": {
		"bs": "readonly",
		"OOJSPlus": "readonly"
	},
	"rules": {
		"max-len": "off",
		"no-prototype-builtins": "off"
	}
}
